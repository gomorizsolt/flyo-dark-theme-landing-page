{
  "name": "flyo-dark-theme-landing-page",
  "version": "1.0.0",
  "description": "Frontend Mentod - Fly Dark Theme Landing Page Challenge",
  "scripts": {
    "build": "cross-env NODE_ENV=production postcss assets/styles/tailwind.css -o assets/styles/tailwind.generated.css",
    "watch:tailwind": "postcss -w assets/styles/tailwind.css -o assets/styles/tailwind.generated.css",
    "dev": "run-p watch:tailwind dev:server",
    "dev:server": "live-server --port=3000 --no-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gomorizsolt/flyo-dark-theme-landing-page.git"
  },
  "keywords": [
    "flyo",
    "frontend",
    "frontend-master",
    "tailwindcss"
  ],
  "author": "Zsolt Gömöri",
  "license": "MIT",
  "homepage": "https://github.com/gomorizsolt/flyo-dark-theme-landing-page",
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "cross-env": "^7.0.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6"
  }
}